{
  "name": "@chrisw-b/astro-watch-watch",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "dev": "astro dev",
    "lint:astro": "astro check",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:js": "eslint ./ --ext .js,.ts,.tsx,.astro",
    "prettier": "prettier --list-different . --write",
    "prettier:check": "prettier . -c",
    "preview": "astro preview",
    "start": "astro dev",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/netlify": "2.1.2",
    "@astrojs/react": "2.0.2",
    "astro": "2.0.10",
    "luxon": "3.2.1",
    "mongodb": "5.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.5",
    "react-string-replace": "1.1.0",
    "remark-gfm": "3.0.1"
  },
  "devDependencies": {
    "@ronilaukkarinen/stylelint-a11y": "1.2.4",
    "@types/luxon": "3.2.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "eslint": "8.34.0",
    "eslint-plugin-astro": "0.23.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "import-sort": "6.0.0",
    "import-sort-parser-typescript": "6.0.0",
    "import-sort-style-react": "5.1.0",
    "prettier": "2.8.4",
    "prettier-plugin-astro": "0.8.0",
    "prettier-plugin-import-sort": "0.0.7",
    "prettier-plugin-packagejson": "2.4.3",
    "stylelint": "14.16.1",
    "stylelint-config-clean-order": "2.3.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "29.0.0",
    "stylelint-no-unsupported-browser-features": "6.0.1"
  },
  "importSort": {
    ".js, .jsx, .es6, .es, .mjs, .cjs": {
      "parser": "babylon",
      "style": "react"
    },
    ".ts, .tsx, .astro": {
      "parser": "typescript",
      "style": "react"
    }
  }
}
