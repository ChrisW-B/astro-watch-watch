{
  "name": "@chrisw-b/astro-watch-watch",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "dev": "astro dev",
    "lint:astro": "astro check",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:js": "eslint ./ --ext .js,.ts,.tsx,.astro",
    "prettier": "prettier --list-different . --write",
    "prettier:check": "prettier . -c",
    "preview": "astro preview",
    "start": "astro dev",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/netlify": "2.2.3",
    "@astrojs/react": "2.1.3",
    "astro": "2.1.9",
    "luxon": "3.3.0",
    "mongodb": "5.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.7",
    "react-string-replace": "1.1.1",
    "remark-gfm": "3.0.1"
  },
  "devDependencies": {
    "@ronilaukkarinen/stylelint-a11y": "1.2.7",
    "@types/luxon": "3.2.2",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "eslint": "8.37.0",
    "eslint-plugin-astro": "0.26.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "import-sort": "6.0.0",
    "import-sort-parser-typescript": "6.0.0",
    "import-sort-style-react": "5.1.0",
    "prettier": "2.8.8",
    "prettier-plugin-astro": "0.8.1",
    "prettier-plugin-import-sort": "0.0.7",
    "prettier-plugin-packagejson": "2.4.5",
    "stylelint": "15.4.0",
    "stylelint-config-clean-order": "5.0.1",
    "stylelint-config-standard": "31.0.0",
    "stylelint-no-unsupported-browser-features": "6.1.0"
  },
  "importSort": {
    ".js, .jsx, .es6, .es, .mjs, .cjs": {
      "parser": "babylon",
      "style": "react"
    },
    ".ts, .tsx, .astro": {
      "parser": "typescript",
      "style": "react"
    }
  }
}
